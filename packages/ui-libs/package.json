{
  "name": "tanstack-shadcn-table",
  "version": "1.1.2",
  "description": "A powerful, feature-rich React table component built on top of TanStack Table v8 with shadcn/ui styling. Optimized bundle size with 55% reduction through peer dependencies.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "style": "./dist/styles.css"
    },
    "./table-elements": {
      "import": "./dist/table-elements.esm.js",
      "require": "./dist/table-elements.js",
      "types": "./dist/table-elements.d.ts"
    },
    "./ui-elements": {
      "import": "./dist/ui-elements.esm.js",
      "require": "./dist/ui-elements.js",
      "types": "./dist/ui-elements.d.ts"
    },
    "./table": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "style": "./dist/styles.css"
    },
    "./styles.css": "./dist/styles.css",
    "./i18n/en": {
      "import": "./dist/i18n/en.esm.js",
      "require": "./dist/i18n/en.js",
      "types": "./dist/i18n/en.d.ts"
    },
    "./i18n/tr": {
      "import": "./dist/i18n/tr.esm.js",
      "require": "./dist/i18n/tr.js",
      "types": "./dist/i18n/tr.d.ts"
    },
    "./i18n/es": {
      "import": "./dist/i18n/es.esm.js",
      "require": "./dist/i18n/es.js",
      "types": "./dist/i18n/es.d.ts"
    },
    "./i18n/fr": {
      "import": "./dist/i18n/fr.esm.js",
      "require": "./dist/i18n/fr.js",
      "types": "./dist/i18n/fr.d.ts"
    },
    "./i18n/de": {
      "import": "./dist/i18n/de.esm.js",
      "require": "./dist/i18n/de.js",
      "types": "./dist/i18n/de.d.ts"
    },
    "./security/sanitize": {
      "import": "./dist/security/sanitize.esm.js",
      "require": "./dist/security/sanitize.js",
      "types": "./dist/security/sanitize.d.ts"
    },
    "./security/validation": {
      "import": "./dist/security/validation.esm.js",
      "require": "./dist/security/validation.js",
      "types": "./dist/security/validation.d.ts"
    },
    "./security/rate-limiter": {
      "import": "./dist/security/rate-limiter.esm.js",
      "require": "./dist/security/rate-limiter.js",
      "types": "./dist/security/rate-limiter.d.ts"
    },
    "./security/csp": {
      "import": "./dist/security/csp.esm.js",
      "require": "./dist/security/csp.js",
      "types": "./dist/security/csp.d.ts"
    },
    "./hooks/use-debounce": {
      "import": "./dist/hooks/use-debounce.esm.js",
      "require": "./dist/hooks/use-debounce.js",
      "types": "./dist/hooks/use-debounce.d.ts"
    },
    "./hooks/use-rate-limit": {
      "import": "./dist/hooks/use-rate-limit.esm.js",
      "require": "./dist/hooks/use-rate-limit.js",
      "types": "./dist/hooks/use-rate-limit.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/styles.css"
  ],
  "scripts": {
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "build:registry": "node scripts/build-registry.js",
    "build:i18n-types": "node scripts/generate-i18n-types.js",
    "build": "NODE_ENV=production npm run build:css && npm run build:registry && rollup -c && npm run build:i18n-types",
    "build:analyze": "ANALYZE=true npm run build:css && ANALYZE=true rollup -c",
    "build:prod": "NODE_ENV=production npm run build:css && npm run build:registry && rollup -c && npm run build:i18n-types",
    "dev": "rollup -c -w",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "npm run build:prod"
  },
  "keywords": [
    "react",
    "react-table",
    "tanstack-table",
    "data-table",
    "table-component",
    "ui-components",
    "tailwind-css",
    "shadcn-ui",
    "typescript",
    "filtering",
    "sorting",
    "pagination",
    "column-resizing",
    "drag-drop",
    "internationalization",
    "i18n"
  ],
  "author": "Osman Ekrem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/osmanekrem/shadecn-ui-libs.git"
  },
  "homepage": "https://github.com/osmanekrem/shadecn-ui-libs#readme",
  "bugs": {
    "url": "https://github.com/osmanekrem/shadecn-ui-libs/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "lucide-react": "^0.474.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tailwind-merge": "^3.3.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2"
  },
  "peerDependenciesMeta": {
    "@dnd-kit/core": {
      "optional": true
    },
    "@dnd-kit/sortable": {
      "optional": true
    },
    "@dnd-kit/modifiers": {
      "optional": true
    },
    "@dnd-kit/utilities": {
      "optional": true
    }
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^2.79.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {}
}
